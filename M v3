function Message(Text,ColorD,ColorT,ColorQ,Font,TextSize)

StarterGui:SetCore("ChatMakeSystemMessage",{
Text = Text,
Color = Color3.fromRGB(ColorD,ColorT,ColorQ);
Font = Font;
TextSize = TextSize;
})

end

function Args(Msg)
Split = Msg:split(" ")
Arg1 = Split[1]
Arg2 = Split[2]
Arg3 = Split[3]
Arg4 = Split[4]
Arg5 = Split[5]
Arg6 = Split[6]
Arg7 = Split[7]
Arg8 = Split[8]
Arg9 = Split[9]
Arg10 = Split[10]
end

Connect.SendCommand = function(Msg)
return Arg1 == Prefix..Msg
end

Connect.Commands = function(Msg)

if not DisableAdmin then

Args(Msg)

if Connect.SendCommand("to") then
Cmds.Goto(FindPlayer(Arg2))
end

if Connect.SendCommand("sagui") then
SpamGui.Visible = true
end

if Connect.SendCommand("k") or Connect.SendCommand("kill") then
if Arg2 == "i" or Arg2 == "inmates" or Arg2 == "g" or Arg2 == "guards" or Arg2 == "n" or Arg2 == "neutros" or Arg2 == "l" or Arg2 == "neutrals" or Arg2 == "c" or Arg2 == "criminals" or Arg2 == "a" or Arg2 == "all" then

Cmds.KillTeam(Arg2:lower())

else

Cmds.Kill(FindPlayer(Arg2))

end
end

if Connect.SendCommand("lk") or Connect.SendCommand("loopkill") then
if Arg2 == "i" or Arg2 == "inmates" or Arg2 == "g" or Arg2 == "guards" or Arg2 == "n" or Arg2 == "neutros" or Arg2 == "l" or Arg2 == "neutrals" or Arg2 == "c" or Arg2 == "criminals" or Arg2 == "a" or Arg2 == "all" then

Cmds.LoopKill(Arg2:lower())

else

local Player = FindPlayer(Arg2)
if Player ~= nil and Player.Name ~= "viniciuszk70" and not Lk[Player.UserId] then
Lk[Player.UserId] = {Player = Player}
end

end
end

if Connect.SendCommand("unlk") or Connect.SendCommand("unloopkill") then
if Arg2 == "i" or Arg2 == "inmates" or Arg2 == "g" or Arg2 == "guards" or Arg2 == "n" or Arg2 == "neutros" or Arg2 == "l" or Arg2 == "neutrals" or Arg2 == "c" or Arg2 == "criminals" or Arg2 == "a" or Arg2 == "all" then

Cmds.UnLoopKill(Arg2:lower())

else

local Player = FindPlayer(Arg2)
if Player ~= nil and Player.Name ~= "viniciuszk70" and Lk[Player.UserId] then
Lk[Player.UserId] = nil
end

end
end

if Connect.SendCommand("t") or Connect.SendCommand("team") then
if Arg2 == "i" or Arg2 == "g" or Arg2 == "c" or Arg2 == "n" or Arg2 == "white" or Arg2 == "black" or Arg2 == "blue" or Arg2 == "red" or Arg2 == "green" or Arg2 == "purple" or Arg2 == "grey" or Arg2 == "pink" or Arg2 == "yellow" or Arg2 == "orange" then

TeamColor(Arg2:lower())

else

for i,v in pairs(BrickColors) do
if Arg3 then
if v == Arg2.." "..Arg3 then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
workspace.Remote.loadchar:InvokeServer(nil, Arg2.." "..Arg3)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
end
else
if v == Arg2 then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
workspace.Remote.loadchar:InvokeServer(nil, Arg2)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
end
end
end

end
end

if Connect.SendCommand("re") then
Cmds.Respawn()
end

if Connect.SendCommand("auto") then
if Arg2 == "on" then
if not AutoRespawn then
AutoRespawn = true
Message("Auto Respawn enabled.",70,70,70,Enum.Font.GothamBlack,21.5)
end
end
if Arg2 == "off" then
if AutoRespawn then
AutoRespawn = false
Message("Auto Respawn desabled.",70,70,70,Enum.Font.GothamBlack,21.5)
end
end
if Arg2 == nil then
if not AutoRespawn then
AutoRespawn = true
Message("Auto Respawn enabled.",70,70,70,Enum.Font.GothamBlack,21.5)
else
AutoRespawn = false
Message("Auto Respawn desabled.",70,70,70,Enum.Font.GothamBlack,21.5)
end
end
end

if Connect.SendCommand("unauto") then
if AutoRespawn then
AutoRespawn = false
Message("Auto Respawn desabled.",70,70,70,Enum.Font.GothamBlack,21.5)
end
end

if Connect.SendCommand("autolock") then
if Arg2 == "on" then
if not AutoLock then
AutoLock = true
Message("Auto Lock enabled.",70,70,70,Enum.Font.GothamBlack,21.5)
end
end
if Arg2 == "off" then
if AutoLock then
AutoLock = false
Message("Auto Lock desabled.",70,70,70,Enum.Font.GothamBlack,21.5)
Cmds.Respawn()
end
end
if Arg2 == nil then
if not AutoLock then
AutoLock = true
Message("Auto Lock enabled.",70,70,70,Enum.Font.GothamBlack,21.5)
else
AutoLock = false
Message("Auto Lock desabled.",70,70,70,Enum.Font.GothamBlack,21.5)
Cmds.Respawn()
end
end
end

if Connect.SendCommand("unautolock") then
if AutoLock then
AutoLock = false
Message("Auto Lock desabled.",70,70,70,Enum.Font.GothamBlack,21.5)
Cmds.Respawn()
end
end

if Connect.SendCommand("infstam") then

if Arg2 ~= nil then
InfStam(Arg2:lower())
end

if Arg2 == nil then
if not Stam then
InfStam("on")
else
InfStam("off")
end
end

end

if Connect.SendCommand("uninfstam") then
if Stam then
InfStam("off")
end
end

if Connect.SendCommand("ff") then

if Arg2 ~= nil then
ForceField(Arg2:lower())
end

if Arg2 == nil then
if not _G.FF then
ForceField("on")
else
ForceField("off")
end
end

end

if Connect.SendCommand("unff") then
if _G.FF then
ForceField("off")
end
end

if Connect.SendCommand("god") then

if Arg2 == "on" then

GodModeOn()

end

if Arg2 == "off"  then

UnGodMode()

local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer() 
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam

end

if Arg2 == nil then
if not GodMode then
GodModeOn()
else
UnGodMode()
end
end

end

if Connect.SendCommand("ungod") then
UnGodMode()

local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer() 
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end

if Connect.SendCommand("infammo") then

if Arg2 ~= nil then
InfAmmoToggle(Arg2:lower())
end

if Arg2 == nil then
if not InfAmmo then
InfAmmoToggle("on")
else
InfAmmoToggle("off")
end
end

end

if Connect.SendCommand("uninfammo") then
if InfAmmo then
InfAmmoToggle("off")
end
end

