StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.All,true)
end
end)
end
end
end)

spawn(function()
while wait() do
if not DisableAdmin then
for i,v in pairs(Sa) do
pcall(function()
if v.Player.Character:FindFirstChild("HumanoidRootPart") then
if v.Player.TeamColor.Name == "Really red" then
for i = 1,100 do
coroutine.resume(coroutine.create(function()
LPlayer.Character.HumanoidRootPart.CFrame = v.Player.Character.HumanoidRootPart.CFrame + v.Player.Character.HumanoidRootPart.CFrame.lookVector * -1.7
workspace.Remote.arrest:InvokeServer(v.Player.Character.Head)
end))
end
else
pcall(function()
Crim(v.Player)
end)
end
end
end)
end
end
end
end)

