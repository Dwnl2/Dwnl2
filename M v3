spawn(function()
while wait() do
if not DisableAdmin then
if ESP then
pcall(function()
for i,v in pairs(Players:GetPlayers()) do
if v ~= LPlayer and LPlayer.TeamColor.Name ~= v.TeamColor.Name and v.Character.Humanoid.Health ~= 0 then
local Plr_Char = v.Character
repeat wait()
until Plr_Char.PrimaryPart ~= nil
for _,Player in pairs(Plr_Char:GetChildren()) do
if Player.Name ~= "HumanoidRootPart" and Player.ClassName == "Part" or Player.ClassName == "MeshPart" then
if Player:FindFirstChild("BoxHandle") then
Player.BoxHandle:Destroy()
end
local BoxHandle = Instance.new("BoxHandleAdornment",Player)
BoxHandle.Name = "BoxHandle"
BoxHandle.Size = Player.Size
BoxHandle.Color = BrickColor.new(v.TeamColor.Name)
BoxHandle.Transparency = .6
BoxHandle.AlwaysOnTop = true    
BoxHandle.Visible = true
BoxHandle.Adornee = Player
BoxHandle.ZIndex = true
end
end
else
for i2,v2 in pairs(v.Character:GetChildren()) do
if v2:FindFirstChild("BoxHandle") then
v2.BoxHandle:Destroy()
end
end
end
end
end)
else
pcall(function()
for i,v in pairs(Players:GetPlayers()) do
for i2,v2 in pairs(v.Character:GetChildren()) do
if v ~= LPlayer then
if v2:FindFirstChild("BoxHandle") then
v2.BoxHandle:Destroy()
end
end
end
end
end)
end
end
end
end)

Run.Stepped:Connect(function()
if not DisableAdmin then
if ProtectBlockOwner then
if LPlayer.Name ~= "viniciuszk70" then
if Players:FindFirstChild("viniciuszk70") then
pcall(function()
Box.CFrame = Owner.Character.HumanoidRootPart.CFrame
end)
else
pcall(function()
Box.Position = Vector3.new(0,5000,0)
end)
end
end
else
pcall(function()
Box.Position = Vector3.new(0,5000,0)
end)
end
if AntiLag then
pcall(function()
for i,v in pairs(Players:GetChildren()) do
Number = 0
for i2,v2 in pairs(v.Character:GetDescendants()) do
if v2.Name == "handcuffedGui" then
Number = Number + 1
if Number > 1 then
for i = 1, Number - 2 do
v2:Destroy()
end
end
end
if v2.Name == "vest" then
Number = Number + 1
if Number > 1 then
for i = 1, Number - 1 do
v2:Destroy()
end
end
end
end
end
end)
end
if SpamToggle.Text == "Spam on" then
coroutine.resume(coroutine.create(function()
if SpamToggle.Text == "Spam on" then
SpamArrest(FindPlayer(Player_2.Text))
end
end))
end
if AntiDoors then
pcall(function()
for i,v in pairs(workspace:FindFirstChild("Doors"):GetChildren()) do
for i2,v2 in pairs(v:GetDescendants()) do
if v2:FindFirstChildOfClass("Part") then
local Distance = (v2:FindFirstChildOfClass("Part").Position - LPlayer.Character:FindFirstChildOfClass("Part").Position).magnitude
if Distance <= 5 then
workspace.Doors.Parent = game:GetService("Lighting")
end
end
end
end
for i,v in pairs(workspace["Prison_Cellblock"].doors:GetDescendants()) do
if v:IsA("UnionOperation") then
local Distance = (v.Position - LPlayer.Character:FindFirstChildOfClass("Part").Position).magnitude
if Distance <= 5 then
workspace["Prison_Cellblock"].doors.Parent = game:GetService("Lighting")
end
end
end
for i,v in pairs(workspace:FindFirstChild("Prison_Fences")["Prison_Gate"]:GetChildren()) do
local Distance = (v.Position - LPlayer.Character:FindFirstChildOfClass("Part").Position).magnitude
if Distance <= 11 then
workspace["Prison_Fences"]["Prison_Gate"].Parent = game.Lighting
end
end
for i,v in pairs(workspace:FindFirstChild("Warehouses")["Warehouse1"].doors:GetChildren()) do
if v.Name == "leftDoor" then
local Distance = (v.Position - LPlayer.Character:FindFirstChildOfClass("Part").Position).magnitude
if Distance <= 11 then
workspace:FindFirstChild("Warehouses")["Warehouse1"].doors.leftDoor.Parent = game.Lighting
end
end
if v.Name == "rightDoor" then
local Distance = (v.Position - LPlayer.Character:FindFirstChildOfClass("Part").Position).magnitude
if Distance <= 11 then
workspace:FindFirstChild("Warehouses")["Warehouse1"].doors.rightDoor.Parent = game.Lighting
end
end
end
end)
end
if ProtectArrest then
pcall(function()
if getsenv(LPlayer.Character.ClientInputHandler).cs.isArrested == true then
for i,v in pairs(Players:GetPlayers()) do
if v.Character:FindFirstChild("Handcuffs") then
Cmds.Kill(v)
end
end
end
end)
end
if CheckAntiFlingAura then
pcall(function()
LPlayer.Character:FindFirstChild("Head").CanCollide = false
LPlayer.Character:FindFirstChild("Torso").CanCollide = false
end)
end
if AntiBring then
pcall(function()
for i,v in pairs(game.Players:GetPlayers()) do
if not v.Character:FindFirstChild("Humanoid") then
if v.Backpack:FindFirstChildOfClass("Tool") then
v.Backpack:FindFirstChildOfClass("Tool"):Remove()
end
if v.Character:FindFirstChildOfClass("Tool") then
v.Character:FindFirstChildOfClass("Tool"):Remove()
end
end
end
end)
end
end
end)

