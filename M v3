end
end
end)

Message("Some things will be activated.",70,70,70,Enum.Font.GothamBlack,21.5)

AutoRespawn = true
Message("Auto Respawn enabled.",70,70,70,Enum.Font.GothamBlack,21.5)

AntiVoid = true
Message("Anti Void enabled.",70,70,70,Enum.Font.GothamBlack,21.5)

AntiShield = true
Message("Anti Shield enabled.",70,70,70,Enum.Font.GothamBlack,21.5)

AntiArrest = true
Message("Anti Arrest enabled.",70,70,70,Enum.Font.GothamBlack,21.5)

ExecuteAntiPunch()
AntiPunch = true
Message("Anti Punch enabled.",70,70,70,Enum.Font.GothamBlack,21.5)

AntiDoors = true
Message("Anti Doors enabled.",70,70,70,Enum.Font.GothamBlack,21.5)

