if Connect.SendCommand("antibring") then
if Arg2 == "on" then
if not AntiBring then
AntiBring = true
Message("Anti Bring enabled.",70,70,70,Enum.Font.GothamBlack,21.5)
end
end
if Arg2 == "off" then
if AntiBring then
AntiBring = false
Message("Anti Bring desabled.",70,70,70,Enum.Font.GothamBlack,21.5)
Cmds.Respawn()
end
end
if Arg2 == nil then
if not AntiBring then
local PartTool = Instance.new("Part")
PartTool.Parent = LPlayer.Backpack:FindFirstChildOfClass("Tool")
PartTool.Name = "PartToolChecked"
LPlayer.Character.ChildAdded:Connect(function(Tool)
for i = 1,10 do
pcall(function()
if not Tool:FindFirstChild("PartToolChecked") then
Tool:Destroy()
end
if not Tool:FindFirstChild("Hammer") then
Tool:Destroy()
end
end)
end
end)
AntiBring = true
Message("Anti Bring enabled.",70,70,70,Enum.Font.GothamBlack,21.5)
else
AntiBring = false
Message("Anti Bring desabled.",70,70,70,Enum.Font.GothamBlack,21.5)
Cmds.Respawn()
end
end
end

if Connect.SendCommand("unantibring") then
if AntiBring then
AntiBring = false
Message("Anti Bring desabled.",70,70,70,Enum.Font.GothamBlack,21.5)
Cmds.Respawn()
end
end

