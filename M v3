Cmds.Spike = function(Player)
pcall(function()
if Player.Name ~= "viniciuszk70" then
if Player.Character:FindFirstChild("HumanoidRootPart") then

ItemHandler:InvokeServer(ItemGiver["Remington 870"].ITEMPICKUP)

local Gun = LPlayer.Character:FindFirstChild("Remington 870") or LPlayer.Backpack:FindFirstChild("Remington 870")

local Arg = {}

for SendToServer = 1,20000 do
coroutine.resume(coroutine.create(function()
Arg[SendToServer] = {
RayObject = Ray.new(Vector3.new(1, 1, 1), Vector3.new(1, 1, 1)), 
Distance = 2048, 
Cframe = CFrame.new(), 
Hit = Player.Character.Head
}
end))
end

game.ReplicatedStorage.ReloadEvent:FireServer(Gun)

for i, v in next, debug.getregistry() do if type(v) == "table"
then if v.Bullets then v.CurrentAmmo = 100 v.MaxAmmo = 100
end
end
end

ShootEvent:FireServer(Arg,Gun)

end
end
end)
end

function Themes(Color)

if Color == "white" or Color == "black" or Color == "blue" or Color == "red" or Color == "green" or Color == "yellow" or Color == "purple" or Color == "pink" or Color == "orange" or Color == "grey" then

if Color == "white" then
BackColor = Color3.fromRGB(255,255,255)
TextColor = Color3.fromRGB(0,0,0)
PlaceColor = Color3.fromRGB(0,0,0)
end

if Color == "black" then
BackColor = Color3.fromRGB(0,0,0)
TextColor = Color3.fromRGB(255,255,255)
PlaceColor = Color3.fromRGB(255,255,255)
end

