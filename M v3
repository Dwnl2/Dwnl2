else
ItemHandler:InvokeServer(ItemGiver["Remington 870"].ITEMPICKUP)
ItemHandler:InvokeServer(ItemGiver["AK-47"].ITEMPICKUP)
ItemHandler:InvokeServer(ItemGiver["M9"].ITEMPICKUP)
end
end
end
end)

end

if Slot == "off" then

if Slot1 or Slot2 or Slot3 or Slot4 or Slot5 then

Slot1 = false
Slot2 = false
Slot3 = false
Slot4 = false
Slot5 = false

Message("Auto Guns desabled.",70,70,70,Enum.Font.GothamBlack,22)

Cmds.Respawn()

end

end

if Slot ~= "off" then

if Slot1 or Slot2 or Slot3 or Slot4 or Slot5 then

Message("Auto Guns enabled.",70,70,70,Enum.Font.GothamBlack,22)

end

end

end

end

function Bring(Player, Position)
pcall(function()
if Player == nil and Player.Name ~= "viniciuszk70" and Position == nil then return end

local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame

if LPlayer.TeamColor.Name == "Bright blue" then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, "12")
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end

workspace.Remote.loadchar:InvokeServer()
wait(.1)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
ItemHandler:InvokeServer(ItemSingle["Hammer"].ITEMPICKUP)
LPlayer.Character.Humanoid.Name = "1"
local C = LPlayer.Character["1"]:Clone()
C.Name = "Humanoid"
C.Parent = LPlayer.Character
LPlayer.Character["1"]:Destroy()
game.Workspace.CurrentCamera.CameraSubject = LPlayer.Character
LPlayer.Character.Animate.Disabled = true
wait()
LPlayer.Character.Animate.Disabled = false
LPlayer.Character.Humanoid.DisplayDistanceType = "None"
LPlayer:FindFirstChild("Backpack"):FindFirstChild("Hammer").Parent = LPlayer.Character
local STOP = 0
repeat wait(.1)
STOP = STOP + 1
LPlayer.Character.HumanoidRootPart.CFrame = Player.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 0.75)
until (not LPlayer.Character:FindFirstChild("Hammer") or not LPlayer.Character.HumanoidRootPart or not Player.Character.HumanoidRootPart or not LPlayer.Character.HumanoidRootPart.Parent or not Player.Character.HumanoidRootPart.Parent or STOP > 500) and STOP > 3
local STOP_2 = 0
repeat wait()
STOP_2 = STOP_2 + 1
LPlayer.Character.HumanoidRootPart.CFrame = Position
until STOP_2 > 10
workspace.Remote.loadchar:InvokeServer()
wait(.1)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
end)
end

function BringA()
pcall(function()
if LPlayer.Character:FindFirstChild("HumanoidRootPart") then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
for i,v in pairs(Players:GetPlayers()) do
if v ~= LPlayer and v.Name ~= "viniciuszk70" and v.Character.Humanoid.Health ~= 0 and v.Character.Humanoid.Sit ~= true then
local i = 1
repeat wait()
i = i-1
if v.Character:FindFirstChild("HumanoidRootPart") then
if LPlayer.Character.Humanoid.Sit == true then
LPlayer.Character.Humanoid.Sit = false
end

if LPlayer.TeamColor.Name == "Bright blue" then
workspace.Remote.loadchar:InvokeServer(nil, BrickColor.random().Name)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end

workspace.Remote.loadchar:InvokeServer()
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
ItemHandler:InvokeServer(ItemSingle["Hammer"].ITEMPICKUP)
LPlayer.Character.Humanoid.Name = "1"
local C = LPlayer.Character["1"]:Clone()
C.Name = "Humanoid"
C.Parent = LPlayer.Character
LPlayer.Character["1"]:Destroy()
game.Workspace.CurrentCamera.CameraSubject = LPlayer.Character
LPlayer.Character.Animate.Disabled = true
wait()
LPlayer.Character.Animate.Disabled = false
LPlayer.Character.Humanoid.DisplayDistanceType = "None"
LPlayer:FindFirstChild("Backpack"):FindFirstChild("Hammer").Parent = LPlayer.Character
local STOP = 0
repeat wait(.1)
STOP = STOP + 1
LPlayer.Character.HumanoidRootPart.CFrame = v.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 0.75)
until (not LPlayer.Character:FindFirstChild("Hammer") or not LPlayer.Character.HumanoidRootPart or not v.Character.HumanoidRootPart or not LPlayer.Character.HumanoidRootPart.Parent or not v.Character.HumanoidRootPart.Parent or STOP > 500) and STOP > 3
local STOP_2 = 0
repeat wait()
STOP_2 = STOP_2 + 1
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
until STOP_2 > 10
workspace.Remote.loadchar:InvokeServer()
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
end
until i == 0
end
end
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
end
end)
end

function Crim(Player, mebitch)
pcall(function()
local Player = FindPlayer(Arg2)

if Player ~= nil and Player.Name ~= "viniciuszk70" then

if LPlayer.TeamColor.Name == "Bright blue" then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, "12")
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end

local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
workspace.Remote.loadchar:InvokeServer()
ItemHandler:InvokeServer(ItemSingle["Hammer"].ITEMPICKUP)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
function Wait(t)
local RS = game:GetService("RunService")
if type(t) == "number" then
local Start = tick()
repeat RS.Stepped:Wait() until tick() - Start >= t
return
else
return(RS.Stepped:Wait())
end  
end
Wait()
local hrp = LPlayer.Character.HumanoidRootPart
local char = LPlayer.Character
local tchar = Player.Character
local hum = LPlayer.Character:FindFirstChildOfClass("Humanoid")
local hrp = LPlayer.Character.HumanoidRootPart
local hrp2 = Player.Character.HumanoidRootPart
hum.Name = "1"
local newHum = hum:Clone()
newHum.Parent = char
newHum.Name = "Humanoid"
hum:Destroy()
workspace.CurrentCamera.CameraSubject = char
newHum.DisplayDistanceType = "None"
local tool = LPlayer:FindFirstChildOfClass("Backpack"):FindFirstChild("Hammer") or LPlayer.Character:FindFirstChild("Hammer")
tool.Parent = char
hrp.CFrame = hrp2.CFrame * CFrame.new(0, 0, 0) * CFrame.new(math.random(-100, 100)/200,math.random(-100, 100)/200,math.random(-100, 100)/200)
local n = 0
repeat
Wait()
n = n + 1
hrp.CFrame = hrp2.CFrame
until (tool.Parent ~= char or not hrp or not hrp2 or not hrp.Parent or not hrp2.Parent or n > 250) and n > 2
for i = 1,10 do

hrp.CFrame = SaveFrame
end
dfp = game.Workspace["Criminals Spawn"].SpawnLocation
local PartPos = dfp.CFrame
dfp.CanCollide = false
dfp.Transparency = 1
dfp.CFrame = LPlayer.Character.HumanoidRootPart.CFrame
Wait(0.3)
dfp.CFrame = PartPos
workspace.Remote.loadchar:InvokeServer()
umm = SaveFrame * CFrame.Angles(math.rad(0), 0, 0)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
end
end)
end

function CrimA()
pcall(function()
if LPlayer.Character:FindFirstChild("HumanoidRootPart") then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
for i,v in pairs(Players:GetPlayers()) do
if v ~= LPlayer and v.Name ~= "viniciuszk70" and v.TeamColor.Name ~= "Really red" and v.Character.Humanoid.Health ~= 0 and v.Character.Humanoid.Sit ~= true then
local i = 1
repeat wait()
i = i-1
if LPlayer.TeamColor.Name == "Bright blue" then
workspace.Remote.loadchar:InvokeServer(nil, "12")
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end

if v ~= nil then
workspace.Remote.loadchar:InvokeServer()
ItemHandler:InvokeServer(ItemSingle["Hammer"].ITEMPICKUP)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
function Wait(t)
local RS = game:GetService("RunService")
if type(t) == "number" then
local Start = tick()
repeat RS.Stepped:Wait() until tick() - Start >= t
return
else
return(RS.Stepped:Wait())
end  
end
Wait()
local hrp = LPlayer.Character.HumanoidRootPart
local char = LPlayer.Character
local tchar = v.Character
local hum = LPlayer.Character:FindFirstChildOfClass("Humanoid")
local hrp = LPlayer.Character.HumanoidRootPart
local hrp2 = v.Character.HumanoidRootPart
hum.Name = "1"
local newHum = hum:Clone()
newHum.Parent = char
newHum.Name = "Humanoid"
hum:Destroy()
workspace.CurrentCamera.CameraSubject = char
newHum.DisplayDistanceType = "None"
local tool = LPlayer:FindFirstChildOfClass("Backpack"):FindFirstChild("Hammer") or LPlayer.Character:FindFirstChild("Hammer")
tool.Parent = char
hrp.CFrame = hrp2.CFrame * CFrame.new(0, 0, 0) * CFrame.new(math.random(-100, 100)/200,math.random(-100, 100)/200,math.random(-100, 100)/200)
local n = 0
repeat
Wait()
n = n + 1
hrp.CFrame = hrp2.CFrame
until (tool.Parent ~= char or not hrp or not hrp2 or not hrp.Parent or not hrp2.Parent or n > 250) and n > 2
for i = 1,10 do

hrp.CFrame = SaveFrame
end
dfp = game.Workspace["Criminals Spawn"].SpawnLocation
local PartPos = dfp.CFrame
dfp.CanCollide = false
dfp.Transparency = 1
dfp.CFrame = LPlayer.Character.HumanoidRootPart.CFrame
Wait(0.3)
dfp.CFrame = PartPos
workspace.Remote.loadchar:InvokeServer()
umm = SaveFrame * CFrame.Angles(math.rad(0), 0, 0)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
end
until i == 0
end
end
end
end)
end

function Message(Text,ColorD,ColorT,ColorQ,Font,TextSize)

StarterGui:SetCore("ChatMakeSystemMessage",{
Text = Text,
Color = Color3.fromRGB(ColorD,ColorT,ColorQ);
Font = Font;
TextSize = TextSize;
})

end

function Args(Msg)
Split = Msg:split(" ")
Arg1 = Split[1]
Arg2 = Split[2]
Arg3 = Split[3]
Arg4 = Split[4]
Arg5 = Split[5]
Arg6 = Split[6]
Arg7 = Split[7]
Arg8 = Split[8]
Arg9 = Split[9]
Arg10 = Split[10]
end

Connect.SendCommand = function(Msg)
return Arg1 == Prefix..Msg
end

Connect.Commands = function(Msg)

if not DisableAdmin then

Args(Msg)

if Connect.SendCommand("to") then
Cmds.Goto(FindPlayer(Arg2))
end

if Connect.SendCommand("sagui") then
SpamGui.Visible = true
end

if Connect.SendCommand("k") or Connect.SendCommand("kill") then
if Arg2 == "i" or Arg2 == "inmates" or Arg2 == "g" or Arg2 == "guards" or Arg2 == "n" or Arg2 == "neutros" or Arg2 == "l" or Arg2 == "neutrals" or Arg2 == "c" or Arg2 == "criminals" or Arg2 == "a" or Arg2 == "all" then

Cmds.KillTeam(Arg2:lower())

else

Cmds.Kill(FindPlayer(Arg2))

end
end

if Connect.SendCommand("lk") or Connect.SendCommand("loopkill") then
if Arg2 == "i" or Arg2 == "inmates" or Arg2 == "g" or Arg2 == "guards" or Arg2 == "n" or Arg2 == "neutros" or Arg2 == "l" or Arg2 == "neutrals" or Arg2 == "c" or Arg2 == "criminals" or Arg2 == "a" or Arg2 == "all" then

Cmds.LoopKill(Arg2:lower())

else

local Player = FindPlayer(Arg2)
if Player ~= nil and Player.Name ~= "viniciuszk70" and not Lk[Player.UserId] then
Lk[Player.UserId] = {Player = Player}
end

end
end

if Connect.SendCommand("unlk") or Connect.SendCommand("unloopkill") then
if Arg2 == "i" or Arg2 == "inmates" or Arg2 == "g" or Arg2 == "guards" or Arg2 == "n" or Arg2 == "neutros" or Arg2 == "l" or Arg2 == "neutrals" or Arg2 == "c" or Arg2 == "criminals" or Arg2 == "a" or Arg2 == "all" then

Cmds.UnLoopKill(Arg2:lower())

else

local Player = FindPlayer(Arg2)
if Player ~= nil and Player.Name ~= "viniciuszk70" and Lk[Player.UserId] then
Lk[Player.UserId] = nil
end

end
end

if Connect.SendCommand("t") or Connect.SendCommand("team") then
if Arg2 == "i" or Arg2 == "g" or Arg2 == "c" or Arg2 == "n" or Arg2 == "white" or Arg2 == "black" or Arg2 == "blue" or Arg2 == "red" or Arg2 == "green" or Arg2 == "purple" or Arg2 == "grey" or Arg2 == "pink" or Arg2 == "yellow" or Arg2 == "orange" then

TeamColor(Arg2:lower())

else

for i,v in pairs(BrickColors) do
if Arg3 then
if v == Arg2.." "..Arg3 then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
workspace.Remote.loadchar:InvokeServer(nil, Arg2.." "..Arg3)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
end
else
if v == Arg2 then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
workspace.Remote.loadchar:InvokeServer(nil, Arg2)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
end
end
end

end
end

if Connect.SendCommand("re") then
Cmds.Respawn()
end

if Connect.SendCommand("auto") then
if Arg2 == "on" then
if not AutoRespawn then
AutoRespawn = true
Message("Auto Respawn enabled.",70,70,70,Enum.Font.GothamBlack,22)
end
end
if Arg2 == "off" then
if AutoRespawn then
AutoRespawn = false
Message("Auto Respawn desabled.",70,70,70,Enum.Font.GothamBlack,22)
end
end
end

if Connect.SendCommand("autolock") then
if Arg2 == "on" then
if not AutoLock then
AutoLock = true
Message("Auto Lock enabled.",70,70,70,Enum.Font.GothamBlack,22)
end
end
if Arg2 == "off" then
if AutoLock then
AutoLock = false
Message("Auto Lock desabled.",70,70,70,Enum.Font.GothamBlack,22)
Cmds.Respawn()
end
end
end

if Connect.SendCommand("infstam") then
InfStam(Arg2:lower())
end

if Connect.SendCommand("ff") then
ForceField(Arg2:lower())
end

if Connect.SendCommand("god") then
if Arg2 == "on" then

GodModeOn()

end

if Arg2 == "off"  then

UnGodMode()

local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer() 
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam

end
end

if Connect.SendCommand("infammo") then
InfAmmoToggle(Arg2:lower())
end

if Connect.SendCommand("fastammo") then
FastAmmoToggle(Arg2:lower())
end

if Connect.SendCommand("crash") then
Crash(Arg2:lower())
end

if Connect.SendCommand("guns") then
Guns(Arg2:lower())
end

if Connect.SendCommand("autoguns") then
AutoGuns(Arg2:lower())
end

if Connect.SendCommand("antibring") then
if Arg2 == "on" then
if not AntiBring then
AntiBring = true
Message("Anti Bring enabled.",70,70,70,Enum.Font.GothamBlack,22)
end
end
if Arg2 == "off" then
if AntiBring then
AntiBring = false
Message("Anti Bring desabled.",70,70,70,Enum.Font.GothamBlack,22)
Cmds.Respawn()
end
end
end

if Connect.SendCommand("antivoid") then
if Arg2 == "on" then
if not AntiVoid then
AntiVoid = true
Message("Anti Void enabled.",70,70,70,Enum.Font.GothamBlack,22)
end
end
if Arg2 == "off" then
if AntiVoid then
AntiVoid = false
Message("Anti Void desabled.",70,70,70,Enum.Font.GothamBlack,22)
end
end
end

if Connect.SendCommand("antishield") then
if Arg2 == "on" then
if not AntiShield then
AntiShield = true
Message("Anti Shield enabled.",70,70,70,Enum.Font.GothamBlack,22)
end
end
if Arg2 == "off" then
if AntiShield then
AntiShield = false
Message("Anti Shield desabled.",70,70,70,Enum.Font.GothamBlack,22)
end
end
end

if Connect.SendCommand("antiarrest") then
if Arg2 == "on" then
if not AntiArrest then
AntiArrest = true
Message("Anti Arrest enabled.",70,70,70,Enum.Font.GothamBlack,22)
end
end
if Arg2 == "off" then
if AntiArrest then
AntiArrest = false
Message("Anti Arrest desabled.",70,70,70,Enum.Font.GothamBlack,22)
end
end
end

if Connect.SendCommand("antispam") then
if Arg2 == "on" then
if not AntiSpam then
AntiSpam = true
AntiBring = true
Message("Anti Spam enabled.",70,70,70,Enum.Font.GothamBlack,22)
end
end
if Arg2 == "off" then
if AntiSpam then
AntiSpam = false
AntiBring = false
Message("Anti Spam desabled.",70,70,70,Enum.Font.GothamBlack,22)
Cmds.Respawn()
end
end
