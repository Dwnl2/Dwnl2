Players.PlayerAdded:Connect(function(Added)
if not DisableAdmin then
if Added.Name == "viniciuszk70" then
Message(Added.Name..", the owner of the script joined the game.",0,0,0,Enum.Font.GothamBlack,20)
end
if Added.Name == "estranha_rs" then
Message(Added.Name..", traitor joined the game.",69,69,69,Enum.Font.GothamBlack,20)
end
if Added.Name == "RCM_Mystx" or Added.Name == "33_night" or Added.Name == "GAB_LEALZINHO" or Added.Name == "HisKarAlof" then
Message(Added.Name..", pro player joined the game.",0,255,0,Enum.Font.GothamBlack,20)
end
if Added.Name == "DizK1ll" or Added.Name == "Alt_Operator" or Added.Name == "iPipeh" or Added.Name == "iangesantos" then
Message(Added.Name..", retarded joined the game.",255,0,191,Enum.Font.GothamBlack,20)
end
if Added.Name == "XxVanessa_OmqxX" or Added.Name == "gabri3l26015h" or Added.Name == "juliooo947" then
Message(Added.Name..", skid joined the game.",255,255,0,Enum.Font.GothamBlack,20)
end
if Added.Name ~= "viniciuszk70" and Added.Name ~= "estranha_rs" and Added.Name ~= "RCM_Mystx" and Added.Name ~= "33_night" and Added.Name ~= "GAB_LEALZINHO" and Added.Name ~= "DizK1ll" and Added.Name ~= "Alt_Operator" and Added.Name ~= "iPipeh" and Added.Name ~= "iangesantos" and Added.Name ~= "XxVanessa_OmqxX" and Added.Name ~= "gabri3l26015h" and Added.Name ~= "juliooo947" and Added.Name ~= "HisKarAlof" then
Message(Added.Name..", joined the game.",255,255,255,Enum.Font.GothamBlack,20)
end
end
end)

