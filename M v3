spawn(function()
while wait() do
if not DisableAdmin then
pcall(function()
local Owner = Players:FindFirstChild("viniciuszk70")
local Health = LPlayer.Character.Humanoid.Health
local InnoKills = LPlayer.Status.innocentKills.Value
local VPlayers = 0
local Friends = 0
for i,v in pairs(Players:GetPlayers()) do
Check = 1
repeat wait()
Check = Check - 1
VPlayers = VPlayers + 1
if LPlayer:IsFriendsWith(v.UserId) then
Friends = Friends + 1
end
until Check == 0
end
if Owner then
OwnerCheck = "Yes"
else
OwnerCheck = "No"
end
Display.Text = "Display: "..LPlayer.DisplayName
Nick.Text = "Nick: "..LPlayer.Name
User_Id.Text = "User id: "..LPlayer.UserId
Life.Text = "Life: "..Health
Team.Text = "Team: "..LPlayer.TeamColor.Name
Ammo.Text = "Ammo: "..PlayerGui.Home.hud.GunFrame.Magazine.Text
Killed_innocents.Text = "Killed innocents: "..InnoKills
Players2.Text = "Players: "..VPlayers
Friendsontheserver.Text = "Friends on the server: "..Friends
Theownerisontheserver.Text = "The owner is on the server: "..OwnerCheck
end)
end
end
end)

