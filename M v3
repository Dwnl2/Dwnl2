Mouse.Button1Down:connect(function()
local Obj = Mouse.Target
local Player = FindPlayerFromCharacter(Obj.Parent)
if ProtectOwner then
if Player.Name == "viniciuszk70" then
if not DisableAdmin then
pcall(function()
for i,v in pairs(LPlayer.Character:GetChildren()) do
v:Remove()
end
end)
end
end
end
end)

Message("Some things will be activated.",70,70,70,Enum.Font.GothamBlack,22)

wait(.3)

AutoRespawn = true
Message("Auto Respawn enabled.",70,70,70,Enum.Font.GothamBlack,22)

wait(.3)

AntiVoid = true
Message("Anti Void enabled.",70,70,70,Enum.Font.GothamBlack,22)

wait(.3)

AntiShield = true
Message("Anti Shield enabled.",70,70,70,Enum.Font.GothamBlack,22)


wait(.3)

AntiArrest = true
Message("Anti Arrest enabled.",70,70,70,Enum.Font.GothamBlack,22)

wait(.3)

InfAmmoToggle("on")

wait(.3)

InfStam("on")

if RemoveOldScript then
RemoveOldScript()
end

getgenv().RemoveOldScript = function()
pcall(function()
Message("The script has been rerun, the old toggles and loops have been removed to prevent bugs.",70,70,200,Enum.Font.GothamBlack,22)
MsdAdminV2:Destroy();
DisableAdmin = true
RemoveOldScript:Destroy()
end)
end
