function InfStam(Toggle)

if Toggle == "on" then
if not Stam then
Stam = true

Message("Infinite Stamina enabled.",70,70,70,Enum.Font.GothamBlack,21.5)

for i,v in next, getgc() do
if type(v) == "function" and getfenv(v).script and getfenv(v).script == LPlayer.Character.ClientInputHandler then 
for i2,v2 in next, debug.getupvalues(v) do 
if type(v2) == "number" then 
debug.setupvalue(v, i2, math.huge)
end
end
end
end

LPlayer.CharacterAdded:Connect(function()
if Stam then
if not DisableAdmin then
wait(.1)
pcall(function()
for i,v in next, getgc() do
if type(v) == "function" and getfenv(v).script and getfenv(v).script == LPlayer.Character.ClientInputHandler then 
for i2,v2 in next, debug.getupvalues(v) do 
if type(v2) == "number" then 
debug.setupvalue(v, i2, math.huge)
end
end
end
end
end)
end
end
end)
end
end

if Toggle == "off" then

if Stam then

Stam = false

Message("Infinite Stamina desabled.",70,70,70,Enum.Font.GothamBlack,21.5)

local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer()
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end
end

end

function ForceField(Toggle)

if Toggle == "on" then
if not _G.FF then

_G.FF = true

Message("Force Field enabled.",70,70,70,Enum.Font.GothamBlack,21.5)

while _G.FF == true do wait()

if not DisableAdmin then

TeamEvent:FireServer("Bright blue")

if LPlayer.TeamColor.Name == "Bright blue" then
FFCop = true
FFCrim = false
end

if LPlayer.TeamColor.Name ~= "Bright blue" then
FFCop = false
wait(.2)
if LPlayer.TeamColor.Name ~= "Bright blue" then
FFCrim = true
end
end

if FFCop then

if not LPlayer.Character:FindFirstChildOfClass("ForceField", true) then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer() 
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end

end

if FFCrim then

if not LPlayer.Character:FindFirstChildOfClass("ForceField", true) then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, "1004") 
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
if LPlayer.TeamColor.Name == "Really red" then
TeamEvent:FireServer("Medium stone grey")
end

end

end
end
end

end
end

if Toggle == "off" then

if _G.FF then
_G.FF = false

Message("Force Field desabled.",70,70,70,Enum.Font.GothamBlack,21.5)

if LPlayer.TeamColor.Name == "Medium stone grey" then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, "12") 
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end
end

end
end

function GodModeOn()

if LPlayer.Character:FindFirstChild("HumanoidRootPart") then
local GodPosition = LPlayer.Character.HumanoidRootPart.CFrame
end

if not GodMode then
Message("God Mode enabled.",70,70,70,Enum.Font.GothamBlack,21.5)
end

GodMode = true

if LPlayer.Character:FindFirstChild("HumanoidRootPart") then
local CurrentHum = LPlayer.Character:WaitForChild("Humanoid")
CurrentHum.Name = "1"

-- New Humanoid
local NewHum = CurrentHum:Clone()
NewHum.Name = "Humanoid"
NewHum.Parent = CurrentHum.Parent
NewHum.DisplayDistanceType = "None"
NewHum.WalkSpeed = 25

CurrentHum:Destroy()

if LPlayer.Character:FindFirstChild("Animate", true) then
LPlayer.Character:FindFirstChild("Animate", true).Disabled = true
LPlayer.Character:FindFirstChild("Animate", true).Disabled = false
end

if LPlayer.Character:FindFirstChild("ForceField", true) then
LPlayer.Character:FindFirstChild("ForceField", true):Destroy()
end

game.Workspace.Camera.CameraType = "Custom" 
game.Workspace.CurrentCamera.CameraSubject = LPlayer.Character:WaitForChild("Humanoid")
end

Run.Stepped:Connect(function()
if GodMode then
if not DisableAdmin then
if LPlayer.Character:FindFirstChild("HumanoidRootPart") then
GodPosition = LPlayer.Character.HumanoidRootPart.CFrame
end
end
end
end)

LPlayer.CharacterAdded:Connect(function()
if GodMode then
if not DisableAdmin then

GodMode = false

wait(.1)

if LPlayer.Character:FindFirstChild("HumanoidRootPart") then
LPlayer.Character.HumanoidRootPart.CFrame = GodPosition
end

GodMode = true

if LPlayer.Character:FindFirstChild("HumanoidRootPart") then
local CurrentHum = LPlayer.Character:WaitForChild("Humanoid")
CurrentHum.Name = "1"

-- New Humanoid
local NewHum = CurrentHum:Clone()
NewHum.Name = "Humanoid"
NewHum.Parent = CurrentHum.Parent
NewHum.DisplayDistanceType = "None"
NewHum.WalkSpeed = 25

CurrentHum:Destroy()

if LPlayer.Character:FindFirstChild("Animate", true) then
LPlayer.Character:FindFirstChild("Animate", true).Disabled = true
LPlayer.Character:FindFirstChild("Animate", true).Disabled = false
end

if LPlayer.Character:FindFirstChild("ForceField", true) then
LPlayer.Character:FindFirstChild("ForceField", true):Destroy()
end

game.Workspace.Camera.CameraType = "Custom" 
game.Workspace.CurrentCamera.CameraSubject = LPlayer.Character:WaitForChild("Humanoid")
end

end
end
end)

end

function UnGodMode()
if GodMode then
GodMode = false

Message("God Mode desabled.",70,70,70,Enum.Font.GothamBlack,21.5)

end
end

function InfAmmoToggle(Toggle)

if Toggle == "on" then
if not InfAmmo then
InfAmmo = true

Message("Infinite Ammo enabled.",70,70,70,Enum.Font.GothamBlack,21.5)

if FastAmmo == true then
FastAmmo = false
Message("Fast Ammo desabled.",70,70,70,Enum.Font.GothamBlack,21.5)

local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer()
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end

Run.Stepped:Connect(function()
if InfAmmo then
if not DisableAdmin then

local j = LPlayer.Character:FindFirstChildOfClass("Tool")
game.ReplicatedStorage.ReloadEvent:FireServer(j)

for i, v in next, debug.getregistry() do
if type(v) == "table" then
if v.Bullets then
v.CurrentAmmo = math.huge v.MaxAmmo = math.huge
end
end
end

end
end
end)
end
end

if Toggle == "off" then
if InfAmmo then
InfAmmo = false

Message("Infinite Ammo desabled.",70,70,70,Enum.Font.GothamBlack,21.5)

local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer()
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end
end

end

function FastAmmoToggle(Toggle)

if Toggle == "on" then

if not FastAmmo then

FastAmmo = true

Message("Fast Ammo enabled.",70,70,70,Enum.Font.GothamBlack,21.5)

if InfAmmo == true then
InfAmmo = false
Message("Infinite Ammo desabled.",70,70,70,Enum.Font.GothamBlack,21.5)

local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer()
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end

Run.Stepped:Connect(function()
if FastAmmo then
if not DisableAdmin then

local j = LPlayer.Character:FindFirstChildOfClass("Tool")
game.ReplicatedStorage.ReloadEvent:FireServer(j)

for i, v in next, debug.getregistry() do
if type(v) == "table" then
if v.Bullets then
v.ReloadTime = -math.huge v.Range = math.huge v.FireRate = -math.huge v.AutoFire = true
end
end
end

end
end
end)
end
end

if Toggle == "off" then
if FastAmmo then
FastAmmo = false

Message("Fast Ammo desabled.",70,70,70,Enum.Font.GothamBlack,21.5)

local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer()
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end
end

end

function Crash(Cmd)

if Cmd == "players" then

local Packets = {}

CrashPlayers = true

Run.Stepped:Connect(function()

for CRP = 1,3 do

coroutine.resume(coroutine.create(function()

if CrashPlayers then

ItemHandler:InvokeServer(ItemGiver["M9"].ITEMPICKUP)

local Gun = LPlayer.Character:FindFirstChild("M9") or LPlayer.Backpack:FindFirstChild("M9")

for SendToServer = 1,100000 do
Packets[SendToServer] = {
Cframe = CFrame.new(),
Hit = workspace:FindFirstChildOfClass("Part")
}
end

ShootEvent:FireServer(Packets,Gun)

end

end))

end

end)

wait()

CrashPlayers = false

end

if Cmd == "sprit" then

local Packets = {}

CrashSprit = true

Run.Stepped:Connect(function()

coroutine.resume(coroutine.create(function()

if CrashSprit then

ItemHandler:InvokeServer(ItemGiver["M9"].ITEMPICKUP)

local Gun = LPlayer.Character:FindFirstChild("M9") or LPlayer.Backpack:FindFirstChild("M9")

for SendToServer = 1,500000 do
Packets[SendToServer] = {
Cframe = CFrame.new(),
Hit = workspace:FindFirstChildOfClass("Part")
}
end

for ReplicateSends = 1,3 do
coroutine.resume(coroutine.create(function()
ShootEvent:FireServer(Packets,Gun)
end))
end

end

end))

end)

wait()

CrashSprit = false

end

if Cmd == "mkan" then

local Packets = {}

CrashMkan = true

Run.Stepped:Connect(function()

coroutine.resume(coroutine.create(function()

if CrashMkan then

ItemHandler:InvokeServer(ItemGiver["M9"].ITEMPICKUP)

local Gun = LPlayer.Character:FindFirstChild("M9") or LPlayer.Backpack:FindFirstChild("M9")

for SendToServer = 1,21.50000 do
coroutine.resume(coroutine.create(function()
Packets[SendToServer] = {
Cframe = CFrame.new(),
Hit = workspace:FindFirstChildOfClass("Part")
}
end))
end

for ReplicateSends = 1,3 do
coroutine.resume(coroutine.create(function()
ShootEvent:FireServer(Packets,Gun)
end))
end

end

end))

end)

wait()

CrashMkan = false

end

