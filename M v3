function AutoGuns(Slot)

if Slot == "slot1" or Slot == "slot2" or Slot == "slot3" or Slot == "slot4" or Slot == "slot5" or Slot == "off" then

if Slot == "slot1" then
Slot1 = true

if Slot2 or Slot3 or Slot4 or Slot5 then

Slot2 = false
Slot3 = false
Slot4 = false
Slot5 = false

Cmds.Respawn()
end

if Slot1 then
if CheckGamePass then
ItemHandler:InvokeServer(ItemGiver["Remington 870"].ITEMPICKUP)
ItemHandler:InvokeServer(ItemGiver["M4A1"].ITEMPICKUP)
ItemHandler:InvokeServer(ItemGiver["AK-47"].ITEMPICKUP)
ItemHandler:InvokeServer(ItemGiver["M9"].ITEMPICKUP)
end
end

LPlayer.CharacterAdded:Connect(function()
if Slot1 then
if not DisableAdmin then
if CheckGamePass then
ItemHandler:InvokeServer(ItemGiver["Remington 870"].ITEMPICKUP)
ItemHandler:InvokeServer(ItemGiver["M4A1"].ITEMPICKUP)
ItemHandler:InvokeServer(ItemGiver["AK-47"].ITEMPICKUP)
ItemHandler:InvokeServer(ItemGiver["M9"].ITEMPICKUP)
else
ItemHandler:InvokeServer(ItemGiver["Remington 870"].ITEMPICKUP)
ItemHandler:InvokeServer(ItemGiver["AK-47"].ITEMPICKUP)
ItemHandler:InvokeServer(ItemGiver["M9"].ITEMPICKUP)
end
end
end
end)

end

if Slot == "slot2" then
Slot2 = true

if Slot1 or Slot3 or Slot4 or Slot5 then

Slot1 = false
Slot3 = false
Slot4 = false
Slot5 = false

Cmds.Respawn()
end

if Slot2 then
if CheckGamePass then
ItemHandler:InvokeServer(ItemGiver["M4A1"].ITEMPICKUP)
ItemHandler:InvokeServer(ItemGiver["Remington 870"].ITEMPICKUP)
ItemHandler:InvokeServer(ItemGiver["AK-47"].ITEMPICKUP)
ItemHandler:InvokeServer(ItemGiver["M9"].ITEMPICKUP)
else
ItemHandler:InvokeServer(ItemGiver["Remington 870"].ITEMPICKUP)
ItemHandler:InvokeServer(ItemGiver["AK-47"].ITEMPICKUP)
ItemHandler:InvokeServer(ItemGiver["M9"].ITEMPICKUP)
end
end

