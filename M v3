LPlayer.CharacterAdded:Connect(function()
if AntiArrest then
if not DisableAdmin then
AntiArrest = false
wait(.1)
if CheckDiedAntiArrest == false then
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
if LPlayer.Character.HumanoidRootPart.Position.Y < 80 then
LPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(960.266663, 99.9899826, 2397.16089, 0.726921022, -2.84909412e-08, 0.686721087, -5.75977843e-09, 1, 4.75853241e-08, -0.686721087, -3.85461334e-08, 0.726921022)
end
AntiArrest = true
else
CheckDiedAntiArrest = false
AntiArrest = true
end
end
end
end)

spawn(function()
while wait() do
if AntiArrest then
if not DisableAdmin then
pcall(function()
if LPlayer.Character:FindFirstChild('HumanoidRootPart') then
if not CheckAntiSpam then
if LPlayer.Character:FindFirstChild("Humanoid").Health ~= 0 then
SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
end
end
if LPlayer.Character.Humanoid.Health == 0 then
CheckDiedAntiArrest = true
end
if LPlayer.Character.Head:FindFirstChild("handcuffedGui") then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer()
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end
end
end)
end
end
end
end)

