local Commands = {
"As the admin is not ready yet, there is",
"no list of commands, when it is ready",
"you will have it."
}

for i,v in pairs(Commands) do
local Clone = CmdText:Clone()
Clone.Text = v
Clone.Name = "CMDS"
Clone.Parent = CmdHandler
end

function FindPlayer(Player)
for i,v in pairs(Players:GetPlayers()) do
if string.find(v.Name:lower(),Player) or string.find(v.DisplayName:lower(),Player) then
return v
end
end
end

local function FindPlayerFromCharacter(Character)
for _, Player in pairs(Players:GetPlayers()) do
if Player.Character == Character then
return Player
end
end
end

Crim_2.MouseButton1Down:connect(function()
CrimSagui(FindPlayer(Player_2.Text))
end)

local function GetPower()
if Power.Text ~= "" or tonumber(Power.Text) then
return(tonumber(Power.Text))
else
return 5
end
end

function CrimSagui(Player,mebitch)
local Player = FindPlayer(Player_2.Text)

if LPlayer.TeamColor.Name == "Bright blue" then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, "12")
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end

if Player ~= nil and Player.Name ~= "viniciuszk70" then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
workspace.Remote.loadchar:InvokeServer()
ItemHandler:InvokeServer(ItemSingle["Hammer"].ITEMPICKUP)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
function Wait(t)
local RS = game:GetService("RunService")
if type(t) == "number" then
local Start = tick()
repeat RS.Stepped:Wait() until tick() - Start >= t
return
else
return(RS.Stepped:Wait())
end  
end
Wait()
local hrp = LPlayer.Character.HumanoidRootPart
local char = LPlayer.Character
local tchar = Player.Character
local hum = LPlayer.Character:FindFirstChildOfClass("Humanoid")
local hrp = LPlayer.Character.HumanoidRootPart
local hrp2 = Player.Character.HumanoidRootPart
hum.Name = "1"
local newHum = hum:Clone()
newHum.Parent = char
newHum.Name = "Humanoid"
hum:Destroy()
workspace.CurrentCamera.CameraSubject = char
newHum.DisplayDistanceType = "None"
local tool = LPlayer:FindFirstChildOfClass("Backpack"):FindFirstChild("Hammer") or LPlayer.Character:FindFirstChild("Hammer")
tool.Parent = char
hrp.CFrame = hrp2.CFrame * CFrame.new(0, 0, 0) * CFrame.new(math.random(-100, 100)/200,math.random(-100, 100)/200,math.random(-100, 100)/200)
local n = 0
repeat
Wait()
n = n + 1
hrp.CFrame = hrp2.CFrame
until (tool.Parent ~= char or not hrp or not hrp2 or not hrp.Parent or not hrp2.Parent or n > 250) and n > 2
for i = 1,10 do

hrp.CFrame = SaveFrame
end
dfp = game.Workspace["Criminals Spawn"].SpawnLocation
local PartPos = dfp.CFrame
dfp.CanCollide = false
dfp.Transparency = 1
dfp.CFrame = LPlayer.Character.HumanoidRootPart.CFrame
Wait(0.3)
dfp.CFrame = PartPos
workspace.Remote.loadchar:InvokeServer()
umm = SaveFrame * CFrame.Angles(math.rad(0), 0, 0)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
end
end

function CrimPlayer2(Player)
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local Status = {}

if Player ~= nil and Player.Name ~= "viniciuszk70" then

Status.Crim2 = true

Run.Stepped:Connect(function()

pcall(function()

if Status.Crim2 then

if not DisableAdmin then

if LPlayer.TeamColor.Name == "Really red" then
TeamEvent:FireServer("Bright orange")
end

local CSpawn = workspace["Criminals Spawn"].SpawnLocation
local CSpawnFrame = CSpawn.CFrame
CSpawn.CanCollide = false
CSpawn.Transparency = 1
CSpawn.Size = Vector3.new(10,10,10)

CSpawn.CFrame = LPlayer.Character.HumanoidRootPart.CFrame

LPlayer.Character.HumanoidRootPart.CFrame = Player.Character.HumanoidRootPart.CFrame

for i = 1,3 do
game.ReplicatedStorage.meleeEvent:FireServer(Player)
end

end

end

end)

end)

if Player.TeamColor.Name == "Really red" or wait(5) then

pcall(function()

Status.Crim2 = false

Status = nil

LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame

TeamEvent:FireServer("Bright orange")

CSpawn.CFrame = CSpawnFrame

TeamEvent:FireServer("Bright orange")

end)

end

end

end

function SpamArrest(Player)

local Power = GetPower()

if Player.Character:FindFirstChild('HumanoidRootPart') then
if Player.Character:FindFirstChild('Head') then
for i = 1, Power, 1 do
coroutine.resume(coroutine.create(function()
if SpamToggle.Text == "Spam on" then
for i = 1, Power, 1 do
coroutine.resume(coroutine.create(function()
workspace.Remote.arrest:InvokeServer(Player.Character.Head)
end))
end
end
end))
end
end
end

end

SpamToggle.MouseButton1Click:Connect(function()
pcall(function()
if SpamToggle.Text == "Spam off" then
local Player = FindPlayer(Player_2.Text)
if Player ~= nil and Player.Name ~= "viniciuszk70" then
LPlayer.Character.HumanoidRootPart.CFrame = FindPlayer(Player_2.Text).Character.HumanoidRootPart.CFrame
SpamArrest(FindPlayer(Player_2.Text))
SpamToggle.Text = "Spam on"
else
SpamToggle.Text = "Not found"
end
else
SpamToggle.Text = "Spam off"
end
end)
end)

Cmds.KillTeam = function(TP)

pcall(function()

if TP == "i" or TP == "inmates" or TP == "g" or TP == "guards" or TP == "n" or TP == "neutros" or TP == "l" or TP == "neutrals" or TP == "c" or TP == "criminals" or TP == "a" or TP == "all" then

if TP == "i" or TP == "inmates" then
NormalKillTeam = true
TS = game.Teams.Inmates
end

if TP == "g" or TP == "guards" then
NormalKillTeam = true
TS = game.Teams.Guards
end

if TP == "n" or TP == "neutros" then
NormalKillTeam = true
TS = game.Teams.Neutral
end

if TP == "l" or TP == "neutrals" then
NormalKillTeam = false
KillNeutrals = true
end

if TP == "c" or TP == "criminals" then
NormalKillTeam = true
TS = game.Teams.Criminals
end

if TP == "a" or TP == "all" then
NormalKillTeam = true
TS = game.Players
end

local Arg = {}

function GetFirePos(Player)
return {
["RayObject"] = Ray.new(Vector3.new(), Vector3.new()),
["Distance"] = 0,
["Cframe"] = CFrame.new(),
["Hit"] = Player.Character.Head
}
end

if NormalKillTeam then
for i,v in pairs(TS:GetPlayers()) do
if v ~= LPlayer and v.Name ~= "viniciuszk70" then
if v.Character:FindFirstChild("HumanoidRootPart") then
if not v.Character:FindFirstChildOfClass("ForceField", true) and v.Character.Humanoid.Health ~= 0 then

if LPlayer.TeamColor.Name == v.TeamColor.Name then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, BrickColor.random().Name)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end

if v.Character.Humanoid.Health > 100 then
for i = 1,9 do
table.insert(Arg,GetFirePos(v))
end
else
for i = 1,5 do
table.insert(Arg,GetFirePos(v))
end
end

end
end
end
end
end

if KillNeutrals then
for i,v in pairs(Players:GetPlayers()) do
if v ~= LPlayer and v.Name ~= "viniciuszk70" then
if v.TeamColor.Name ~= "Bright orange" and v.TeamColor.Name ~= "Medium stone grey" and v.TeamColor.Name ~= "Bright blue" and v.TeamColor.Name ~= "Really red"  then
if v.Character:FindFirstChild("HumanoidRootPart") then
if not v.Character:FindFirstChildOfClass("ForceField", true) and v.Character.Humanoid.Health ~= 0 then

if LPlayer.TeamColor.Name == v.TeamColor.Name then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, BrickColor.random().Name)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end

for i = 1,5 do
table.insert(Arg,GetFirePos(v))
end

end
end
end
end
end
end

ItemHandler:InvokeServer(ItemGiver["Remington 870"].ITEMPICKUP)

local Gun = LPlayer.Character:FindFirstChild("Remington 870") or LPlayer.Backpack:FindFirstChild("Remington 870")

game.ReplicatedStorage.ReloadEvent:FireServer(Gun)

for i, v in next, debug.getregistry() do
if type(v) == "table" then
if v.Bullets then
v.CurrentAmmo = 100 v.MaxAmmo = 100
end
end
end

ShootEvent:FireServer(Arg,Gun)

NormalKillTeam = false
KillNeutrals = false

end

end)
end

Cmds.Kill = function(Player)
pcall(function()
if Player.Name ~= "viniciuszk70" then
if Player.Character:FindFirstChild("HumanoidRootPart") then
if not Player.Character:FindFirstChildOfClass("ForceField", true) and Player.Character.Humanoid.Health ~= 0 then

if Player.TeamColor.Name == LPlayer.TeamColor.Name then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, BrickColor.random().Name)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end

ItemHandler:InvokeServer(ItemGiver["Remington 870"].ITEMPICKUP)

local Gun = LPlayer.Character:FindFirstChild("Remington 870") or LPlayer.Backpack:FindFirstChild("Remington 870")

local Arg = {}

local AFire = {
["RayObject"] = Ray.new(Vector3.new(), Vector3.new()), 
["Distance"] = 0, 
["Cframe"] = CFrame.new(), 
["Hit"] = Player.Character.Head
}

if Player.Character.Humanoid.Health > 100 then
for i = 1,9 do
table.insert(Arg,AFire)
end
else
for i = 1,5 do
table.insert(Arg,AFire)
end
end

game.ReplicatedStorage.ReloadEvent:FireServer(Gun)

for i, v in next, debug.getregistry() do if type(v) == "table"
then if v.Bullets then v.CurrentAmmo = 100 v.MaxAmmo = 100
end
end
end

ShootEvent:FireServer(Arg,Gun)

end
end
end
end)
end

Cmds.LoopKill = function(TP)

if TP == "i" or TP == "inmates" or TP == "g" or TP == "guards" or TP == "n" or TP == "neutros" or TP == "l" or TP == "neutrals" or TP == "c" or TP == "criminals" or TP == "a" or TP == "all" then

if TP == "i" or TP == "inmates" then

_G.i = true

while _G.i == true do wait()
if not DisableAdmin then
Cmds.KillTeam("i")
end
end

end

if TP == "g" or TP == "guards" then

_G.g = true

while _G.g == true do wait()
if not DisableAdmin then
Cmds.KillTeam("g")
end
end

end

if TP == "n" or TP == "neutros" then

_G.n = true

while _G.n == true do wait()
if not DisableAdmin then
Cmds.KillTeam("n")
end
end

end

if TP == "l" or TP == "neutrals" then

_G.l = true

while _G.l == true do wait()
if not DisableAdmin then
Cmds.KillTeam("l")
end
end

end

if TP == "c" or TP == "criminals" then

_G.c = true

while _G.c == true do wait()
if not DisableAdmin then
Cmds.KillTeam("c")
end
end

end

if TP == "a" or TP == "all" then

_G.a = true

while _G.a == true do wait()
if not DisableAdmin then
Cmds.KillTeam("a")
end
end

end

end
end

Cmds.UnLoopKill = function(TP)

if TP == "i" or TP == "inmates" or TP == "g" or TP == "guards" or TP == "n" or TP == "neutros" or TP == "l" or TP == "neutrals" or TP == "c" or TP == "criminals" or TP == "a" or TP == "all" then

if TP == "i" or TP == "inmates" then

_G.i = false

end

if TP == "g" or TP == "guards" then

_G.g = false

end

if TP == "n" or TP == "neutros" then

_G.n = false

end

if TP == "l" or TP == "neutrals" then

_G.l = false

end

if TP == "c" or TP == "criminals" then

_G.c = false

end

if TP == "a" or TP == "all" then

_G.a = false

end

end
end

Cmds.Goto = function(Player)
pcall(function()
if Player ~= nil then
LPlayer.Character.HumanoidRootPart.CFrame = Player.Character.HumanoidRootPart.CFrame
end
end)
end

Cmds.Respawn = function()
if LPlayer.TeamColor.Name == "Bright orange" then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer()
wait(.1)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
else
if LPlayer.TeamColor.Name ~= "Medium stone grey" then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer()
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end
end
if LPlayer.TeamColor.Name == "Medium stone grey" then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
TeamEvent:FireServer("Bright orange")
workspace.Remote.loadchar:InvokeServer()
TeamEvent:FireServer("Medium stone grey")
wait(.1)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end
end

function TeamColor(Team)

if Team == "i" then
TeamEvent:FireServer("Bright orange")
end

if Team == "g" then
TeamEvent:FireServer("Bright blue")
end

if Team == "c" then
Bruh = workspace["Criminals Spawn"].SpawnLocation
Bruh.CanCollide = false
Bruh.Transparency = 1
Bruh.CFrame = LPlayer.Character.HumanoidRootPart.CFrame
wait()
Bruh.CanCollide = true
Bruh.Transparency = 0
Bruh.CFrame = CFrame.new(-920.510803, 92.2271957, 2138.27002, 0, 0, -1, 0, 1, 0, 1, 0, 0)
end

if Team == "n" then
TeamEvent:FireServer("Medium stone grey")
end

if Team == "white" then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, "1001")
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
