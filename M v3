ProtectArrest = true
Message("Protect Arrest enabled.",70,70,70,Enum.Font.GothamBlack,21.5)

InfAmmoToggle("on")

InfStam("on")

LoadedTheAdmin = true

if RemoveOldScript then
RemoveOldScript()
end

getgenv().RemoveOldScript = function()
pcall(function()
Message("The script has been rerun, the old toggles and loops have been removed to prevent bugs.",70,70,200,Enum.Font.GothamBlack,21.5)
Cmds.Respawn()
MsdAdminV2:Destroy()
ThemesGui:Destroy()
DisableAdmin = true
RemoveOldScript:Destroy()
end)
end
